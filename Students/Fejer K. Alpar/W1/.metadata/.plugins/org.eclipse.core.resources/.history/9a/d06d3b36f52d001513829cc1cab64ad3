
public class sumultipleTwist2 {

	public static void main(String[] args) {
		int a = 1000, b = 0, i, sum = 0;
		while (b == 0) {
			for (i = 3; i < a; i++)
				if (i % 3 == 0 || i % 5 == 0)
					try {
						sum += i;
					} 
					catch (ArithmeticException e) {
						System.out.println("Int overflow for " + a);
						b++;
					}
			a+=10000;
		}

	}

	
}