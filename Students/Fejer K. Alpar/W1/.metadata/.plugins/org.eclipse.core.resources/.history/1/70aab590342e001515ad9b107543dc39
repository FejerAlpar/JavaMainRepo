
public class palindrome {
public static void main(String[] args) {
	int a=999, b=999, c = 0, p = 1 ;
	while (c==0){
		
	
	while (palind(a*b) != 0)
		b--;}
	if (a*b > p) p = a * b;
	else c++;
	a--;
}
public static int palind(int a){
	int b = 0, c = a;
	while (c!=0) {
		b = b * 10 + c % 10;
		c = c / 10;
	}
	if (a == b) return 1;
	else return 0;
}
}